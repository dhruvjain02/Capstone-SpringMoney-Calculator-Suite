{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs18.x"
      }
    },
    "env": {
      "NEXT_PUBLIC_API_URL": "https://your-api-url.com"
    },
    "github": {
      "enabled": true,
      "autoJobCancelation": true,
      "silent": false
    }
  }
  